#!/bin/bash

TIMEFORMAT=$' (%2lR real)'

echo -n "decom (slice, split=label): "
cd test/decom
time ./runtests -slice on -split label
cd ../..

echo -n "decom (slice, split=pivot): "
cd test/decom
time ./runtests -slice on -split pivot
cd ../..

echo -n "decom (label): "
cd test/decom
time ./runtests -slice off
cd ../..

echo -n "frob: "
cd test/frob
time ./runtests
cd ../..

echo -n "intersect: "
cd test/intersect
time ./runtests
cd ../..

echo -n "assprimes: "
cd test/assprimes
time ./runtests
cd ../..

echo -n "minimize: "
cd test/minimize
time ./runtests
cd ../..
