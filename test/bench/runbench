echo " *** Action optimize on n11d11.test (ref. time on Angel: 16.0s)"
time cat test/bench/n11d11.test test/bench/n11d11|bin/frobby optimize \
  -displayLevel 0 -chopFirstAndSubtract -maxStandard -displayValue off $*

echo " *** Action optimize on sqfreeV25G1000.test (ref. time on Angel: ?s)"
time bin/frobby optimize < sqfreeV25G1000.test -displayLevel 0 -displayValue off $*

echo " *** Action optimize on nearGenericV15G50.test (ref. time on Angel: ?s) "
time bin/frobby optimize < nearGenericV15G50.test -displayLevel 0 \
 -displayValue off $*


